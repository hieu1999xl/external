height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<!-- End Google Tag Manager (noscript) -->
<!-- Google Tag Manager (noscript) -->
<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-MFVGW35"
height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<!-- End Google Tag Manager (noscript) -->
<header class="mypage-header">
    <div class="header-wrapper">
        <nav class="header-item">
            <div class="div-pc">
                <p class="header-logo header-logo-prepaid">
                    <a href="{{ mypage_top_url }}">{{ asset_cdn_img('zeus-prepaid-logo.svg',{'width': 154, 'height': 45, 'alt': 'ZEUS WiFi CHARGE'}) }}</a>
                </p>
            </div>
            <div class="div-sp">
                <p class="header-logo">
                    <a href="{{ mypage_top_url }}">{{ asset_cdn_img('zeus-prepaid-logo.svg',{'width': 118, 'height': 33, 'alt': 'ZEUS WiFi CHARGE'}) }}</a>
                </p>
            </div>
            <div class="header-title">
                <div class="current-tab-name-box">
                    <p class="current-tab-name">
                        {% block current_tab %}
                        {% endblock %}
                    </p>
                </div>
            </div>
            {% if is_login %}
            <div class="mypage-header-logout">
                <div class="header-logout">
                    <a href="{{ logout_url }}">
                        <div class="header-logout-person header-logout-person-black">
                            {{ asset_cdn_img('mypage-logout-black_t.png',{'aria-hidden':'true', 'width': 24, 'height': 25, 'alt':'LOG OUT'}) }}
                        </div>
                        <p>LOG OUT</p>
                        <div class="header-logout-arrow header-logout-arrow-black">
                            {{ asset_cdn_img('mypage-arrow-black_t.png',{'aria-hidden':'true', 'width': 10, 'height': 18, 'alt':'矢印'}) }}
                        </div>
                    </a>
                </div>
            </div>
            {% endif %}
        </nav>
    </div>
</header>
    <div id="mypage-nav-wrapper" class="mypage-nav-wrapper">
        <div class="mypage-nav-arrow mypage-nav-arrow-left mypage-nav-arrow-left-prepaid">
            {{ asset_cdn_img('button-arrow-left_t.png',{'aria-hidden':'true', 'width': 40, 'height': 40, 'alt':'矢印'}) }}
        </div>
        <div class="mypage-nav-arrow mypage-nav-arrow-right mypage-nav-arrow-right-prepaid">
            {{ asset_cdn_img('button-arrow-right_t.png',{'aria-hidden':'true', 'width': 40, 'height': 40, 'alt':'矢印'}) }}
        </div>

    {# マイページのヘッダメニュー #}
    {% if hide_header_menu != true %}
        {% include 'common/mypage/head_menu_prepaid.twig' %}
    {% endif %}

    </div>

    <main class="mypage-main mypage-contents mypage-prepaid-main {{ theme_class }}" role="main">
        <p class="scroll-hint-text scroll-hint--show">スクロールできます→</p>
        <div id="container">
            <div class="mypage-container bg-plain-dark-gray">
            {% block initial_cancel_process_bar %}{% endblock %}
                <div class="mypage-section-container {% if is_maya_installment_plan %}prepaid-content-area{% endif %}">
                    <div class="mypage-title-area">
                        <div class="mypage-title-area-icon">
                            {% block header_icon %}{% endblock %}
                        </div>
                        <div class="mypage-titile-area-text">
                            <h1 class="mypage-titile-area-title">
                                {% block title %}{% endblock %}
                            </h1>
                            <div class="div-sp">
                                <p class="mypage-titile-area-title-sub">
                                    {% block sub_title %}{% endblock %}
                                </p>
                            </div>
                        </div>
                    </div>
                <div class="mypage-message">
                    {% block description %}{% endblock %}
                </div>
                <div class="mypage-error-message Prepaid-error-end">
                    {% include 'common/message.twig' %}
                </div>
                {% block content %}{% endblock %}
                </div>
            </div>
        </div>
    </main>
    <footer class="mypage-footer">
        <div class="mypage-footer-main">
            <div class="wrapper">
                <div>
                    <ul>
                        <li><a href="{{ prepaid_lp_url }}" target="_blank">TOP</a></li>
                        <li><a href="{{ terms_prepaid_url }}" target="_blank">利用規約</a></li>
                        <li><a href="{{ pdf_privacy_url }}" target="_blank">プライバシーポリシー</a></li>
                        <li class="mypage-footer-copyright footer-copy">&copy; 2020 ZEUS WiFi</li>
                    </ul>
                </div>
                <div class="mypage-footer-copyright-sp">
                    &copy; 2020 ZEUS WiFi
                </div>
            </div>
        </div>
    </footer>
</body>
